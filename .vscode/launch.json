{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run server",
      "preLaunchTask": "preLaunchTask",
      "envFile": "${workspaceFolder}/.env",
      "program": "${workspaceFolder}/src/index.ts",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "console": "internalConsole",
      "outputCapture": "std",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}